<h1>Login In</h1>
<%= form_tag sessions_path do %>
	<p>
		<%=label_tag :email %><br />
		<%= text_field_tag :email, params[:email] %>
	</p><br/>
	<p>
		<%=label_tag :password %><br />
		<%=password_field_tag :password %>
	</p><br/>
	<p>
	<%= link_to "forget password?", new_password_reset_path %></p><br/>

	<div class="field">
	<%= label_tag :remember_me %>
